    "objects": [],
    "instanceObjects": [
        {
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id": "info.lastSync",
            "type": "state",
            "common": {
                "name": "Last successful sync",
                "type": "string",
                "role": "value.datetime",
                "read": true,
                "write": false,
                "desc": "Last successful synchronization"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "name": "sonnen connected",
                "type": "boolean",
                "role": "indicator.connected",
                "read": true,
                "write": false,
                "def": false,
                "desc": "Is sonnen battery connected?"
            },
            "native": {}
        },
        {
            "_id": "status",
            "type": "channel",
            "common": {
                "name": "Status"
            },
            "native": {}
        },
        {
            "_id": "status.consumption",
            "type": "state",
            "common": {
                "name": "Consumption",
                "type": "number",
                "role": "value.consumption",
                "read": true,
                "write": false,
                "desc": "House comsumption in watts",
                "unit": "watts"
            },
            "native": {}
        },
        {
            "_id": "status.production",
            "type": "state",
            "common": {
                "name": "Production",
                "type": "number",
                "role": "value.production",
                "read": true,
                "write": false,
                "desc": "Production in watts",
                "unit": "watts"
            },
            "native": {}
        },
        {
            "_id": "status.relativeSoc",
            "type": "state",
            "common": {
                "name": "Relative State of Charge",
                "type": "number",
                "role": "value.rsoc",
                "read": true,
                "min": 0,
                "max": 100,
                "write": false,
                "desc": "State of charge in percent",
                "unit": "%"
            },
            "native": {}
        }
    ]
